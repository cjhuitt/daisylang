foo = { 1 => none, 2 => true, 3 => false }
squares = { 
    1 => 1 ^ 2, 
    2 => 2 ^ 2,
    3 => 3 ^ 2,
    4 => 4 ^ 2
}

print( foo )

for entry in squares
    print( entry )

more = squares + { 5 => 5 ^ 2 }
more.append!( { 6 => 6 ^ 2, 7 => 7 ^ 2 } )
for key in more.keys()
    print( key )
for value in more.values()
    print( value )

