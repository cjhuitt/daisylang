Class: SampleError is Throwable, Stringifiable
    Method: String message()
        return "Sample"
    
    Method: String toString()
        return message()

Method: Throws()
    throw SampleError.create()

try
    Throws()
handle SampleError as err
    print( "Sample Error" )
handle
    print( "Unknown Error" )

try
    Throws()
handle as err
    print( err )

